{
  "name": "tripletexjs",
  "version": "3.1.0-next.3",
  "description": "Typescript / Javascript SDK for Tripletex 2.0 API",
  "scripts": {
    "build": "tsc",
    "generate": "openapi -c node --useOptions -i https://tripletex.no/v2/swagger.json --output src/generated",
    "generate-docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "keywords": [
    "tripletex",
    "accounting",
    "invoice",
    "rest-api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bjerkio/tripletexjs.git"
  },
  "author": "Bjerk AS",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bjerkio/tripletexjs/issues"
  },
  "homepage": "https://github.com/bjerkio/tripletexjs#readme",
  "devDependencies": {
    "@types/jest": "~26.0.10",
    "@types/node": "~14",
    "@types/node-fetch": "^2.5.7",
    "jest": "~26.4.2",
    "openapi-typescript-codegen": "^0.5.4",
    "prettier": "~2.0.5",
    "typedoc": "^0.18.0",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^4.0.5"
  },
  "volta": {
    "node": "14.15.0",
    "npm": "6.14.8",
    "yarn": "1.22.10"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.1"
  }
}
